import java.util.Scanner;

public class projectsCreation_07 {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        String name = scan.nextLine();
        int numberProjects = Integer.parseInt(scan.nextLine());

        int hourWork = numberProjects * 3;

        System.out.printf("The architect %s will need %d " +
                "hours to complete %d project/s.", name, hourWork, numberProjects);
    }
}
