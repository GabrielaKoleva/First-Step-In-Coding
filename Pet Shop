import java.util.Scanner;

public class petShop_08 {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        int foodForDogs = Integer.parseInt(scan.nextLine());
        int catFood = Integer.parseInt(scan.nextLine());

        double priceForDogFood = 2.5 * foodForDogs;
        double priceForCatFood = 4 * catFood;

        double allPriceForFood = priceForDogFood + priceForCatFood;

        System.out.printf("%.1f lv.", allPriceForFood);
    }
}
